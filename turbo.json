{
  "$schema": "https://turborepo.org/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "dev": {
      "cache": false
    },
    "test": {
      "dependsOn": ["^test"],
      "outputs": []
    },
    "exports": {
      "dependsOn": ["^exports"],
      "outputs": []
    }
  }
}
